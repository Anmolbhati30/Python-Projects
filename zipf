		'u':0,
		'v':0,
		'w':0,
		'x':0,
		'y':0,
		'z':0
	}


	for i in alphabet:
		for x in sample_text_array:
			if x == i:
				alphabet[i] += 1
	for i in alphabet:
		print(i,"=", alphabet[i])

